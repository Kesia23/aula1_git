package application;

import java.util.Scanner;

public class Program_Matriz {

	public static void main(String[] args) {
		Scanner sc =new Scanner(System.in);
		
		int n = sc.nextInt();
		int [][] mat=new int[n][n];
		for(int i=0;i<n;i++) {//da linha
			for(int j=0;j<n;j++) { //da coluna
				mat[i][j]=sc.nextInt();	
			}
		}
		
		sc.close();

	}

}
